[include]
  path = .dotfiles/secret/.gitconfig

[core]
  excludesfile = ~/.gitignore
  editor       = /usr/bin/nano

[alias]

[color]
  branch = auto
  diff   = auto
  status = auto
  interactive = auto
  ui     = true

[push]
  default = current

[branch "master"]
  remote = origin
  merge  = refs/heads/master
